{
  "name": "Lofi Tab",
  "description": "New tab to relax/study to",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["geolocation"],
  "chrome_url_overrides": {
    "newtab": "./dist/index.html"
  },
  "icons": {
    "16": "src/images/lofitab16.png",
    "32": "src/images/lofitab32.png",
    "48": "src/images/lofitab48.png",
    "128": "src/images/lofitab128.png"
  }
}
